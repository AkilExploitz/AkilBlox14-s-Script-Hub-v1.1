 local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

  local Window = Rayfield:CreateWindow({
	Name = "Uranium Hub",
	LoadingTitle = "Uranium Hub",
	LoadingSubtitle = "by HamsterShowcaser",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Uranium Hub"
	},
        Discord = {
        	Enabled = true,
        	Invite = "K6JDqbwgc8", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "Uranium Hub Key System",
		Subtitle = "Key System",
		Note = "The Key Will Be Saved! No Worries!",
		FileName = "SiriusKey",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "Supernovae"
	}
  })

 Rayfield:Notify({
    Title = "Uranium Hub",
    Content = "Made by HamsterShowcaser. Have A Nice Day!",
    Duration = 5,
    Image = 4483362458,
